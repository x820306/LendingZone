		if(value1R>0){
			value5=value2R/value1R*100;
		}else{
			value5=0;
		}
		$("#value1").text(Math.round(value1R).toFixed(0));
		$("#value2").text(Math.round(value2R).toFixed(0));
		$("#value3").text(Math.round(value3R).toFixed(0));
