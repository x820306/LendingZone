		gg=1;
	}else{
		gg=parseInt(g);
	}
	var hh;
	if(h===''){
		hh=1;
	}else{
		hh=parseInt(h);
	}
	var ii;
	if(i===''){
		ii=1;
	}else{
		ii=parseFloat(i);
	}
	
	var keepFlag=true;
	var period=$("select[name='AutoComfirmToBorrowMsgPeriod']").val();
	if(period>0){
		var sorter=$("select[name='AutoComfirmToBorrowMsgSorter']").val();
		var lbound=$("input[name='AutoComfirmToBorrowMsgLbound']").val().toString().trim();
		var ubound=$("input[name='AutoComfirmToBorrowMsgUbound']").val().toString().trim();
		if(lbound!==''){
			if((sorter==='Updated')||(sorter==='Created')){
				var tester=Date.parse(lbound);
				if(isNaN(tester)){
					keepFlag=false;
				}
			}else if((sorter==='InterestRate')||(sorter==='InterestInFutureDivMoney')){
				var tester=parseFloat(lbound);
				if((tester<0)||(tester>99)){
					keepFlag=false;
				}else{
					$("input[name='AutoComfirmToBorrowMsgLbound']").val(tester);
				}
			}else{
				var tester=parseInt(lbound);
				if(tester<0){
					keepFlag=false;
				}else{
					$("input[name='AutoComfirmToBorrowMsgLbound']").val(tester);
				}
			}
		}
		
		if(ubound!==''){
			if((sorter==='Updated')||(sorter==='Created')){
				var tester=Date.parse(ubound);
				if(isNaN(tester)){
					keepFlag=false;
				}
			}else if((sorter==='InterestRate')||(sorter==='InterestInFutureDivMoney')){
				var tester=parseFloat(ubound);
				if((tester<0)||(tester>99)){
					keepFlag=false;
				}else{
					$("input[name='AutoComfirmToBorrowMsgUbound']").val(tester);
				}
			}else{
				var tester=parseInt(ubound);
				if(tester<0){
					keepFlag=false;
				}else{
					$("input[name='AutoComfirmToBorrowMsgUbound']").val(tester);
				}
			}
		}
	}
	
	if(flag){
		if((a!=='')&&(b!=='')&&(c!=='')&&(aa>=1)&&(bb>=0.01)&&(bb<=99)&&(cc>=1)&&(cc<=36)&&(dd>=1)&&(ee>=1)&&(ff>=1)&&(gg>=1)&&(hh>=1)&&(ii>=0.01)&&(ii<=99)&&(keepFlag)){
			$("input[name='MaxMoneyToLend']").val(aa);
			$("input[name='InterestRate']").val(bb);
			$("input[name='MonthPeriod']").val(cc);
			if(d!==''){
				$("input[name='MinLevelAccepted']").val(dd);
			}
			if(e!==''){
				$("input[name='MinInterestInFuture']").val(ee);
			}
			if(f!==''){
				$("input[name='MinMoneyFuture']").val(ff);
			}
			if(g!==''){
				$("input[name='MinInterestInFutureMonth']").val(gg);
			}
			if(h!==''){
				$("input[name='MinInterestInFutureMoneyMonth']").val(hh);
			}
			if(i!==''){
				$("input[name='MinInterestInFutureDivMoney']").val(ii);
			}
			
			$(cntNumber).html('<br>'+cntTail+'<br><br>');
			$(target).modal('show');
		}else{
			$('#msgModal').modal('show');
		}
	}else{
		$(cntNumber).html('<br>'+cntTail+'<br><br>');
		$(target).modal('show');
	}
}

<% if(!jsonLend){ %>
	function create(){
		$('#chkModal1').modal('hide');
	}
	
	var submitFlag=false;
	function disableButton(){
		if(!submitFlag){
			submitFlag=true;
			$('#createBtnID').addClass('disabled');
			return true;
		}else{
			return false;
		}
	}
<% }else{%>
	function update(){
		$('#chkModal2').modal('hide');
	}
	
	var submitFlag=false;
	function disableButton(){
		if(!submitFlag){
			submitFlag=true;
			$('#destroyBtnID').addClass('disabled');
			$('#updateBtnID').addClass('disabled');
			
			return true;
		}else{
			return false;
		}
	}
	
	function destroy(){
		$('#chkModal3').modal('hide');
		disableButton();
		
		var form=document.getElementById('formID');
		form.action="/Lends/destroy";
		form.submit();
	}
<% }%>
</script>
