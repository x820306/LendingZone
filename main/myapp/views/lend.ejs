			<%for(i=0;i<lfJSON.Target.length;i++){%>
				<%if(lfJSON.Target[i]){%>
					<%if(i===9){%>
						$('#periodSel').focus();
					<%}else if(i===10){%>
						$('#classorSel').focus();
					<%}else if(i===11){%>
						$('#sorterSel').focus();
					<%}else if(i===12){%>
						$('#directorSel').focus();
					<%}else if(i===13){%>
						$('#lboundSel').focus();
					<%}else if(i===14){%>
						$('#uboundSel').focus();
					<%}else if(i===15){%>
						$('#keywordSel').focus();
					<%}else{%>
						$('#f<%=i+1%>').focus();
					<%}%>
					<%break;%>
				<%}%>
			<%}%>
		<%}%>
		
		if($('#periodSel').val()>0){
			if(($('#sorterSel').val()==='Updated')||($('#sorterSel').val()==='Created')){
				enableDaterInner();
			}else{
				disableDaterInner();
			}
		}else{
			disableDaterInner();
		}
		autoSorter($('#lboundSel').val(),$('#uboundSel').val());
		autoChanger();
	});

function autoSorter(defaultValuel,defaultValueu){
	$('#lboundSel').val(defaultValuel);
	$('#uboundSel').val(defaultValueu);
	if(($('#sorterSel').val()==='Updated')||($('#sorterSel').val()==='Created')){
		$('#lbd').html('');
		$('#ubd').html('');
		enableDater();
	}else{
		if(($('#sorterSel').val()==='InterestInFutureDivMoney')||($('#sorterSel').val()==='InterestRate')){
			$('#lbd').html('%');
			$('#ubd').html('%');
		}else if($('#sorterSel').val()==='MonthPeriod'){
			$('#lbd').html('個月');
			$('#ubd').html('個月');
		}else if($('#sorterSel').val()==='Level'){
			$('#lbd').html('級');
			$('#ubd').html('級');
		}else if(($('#sorterSel').val()==='InterestInFutureMoneyMonth')||($('#sorterSel').val()==='InterestInFutureMonth')){
			$('#lbd').html('元/月');
			$('#ubd').html('元/月');
		}else{
			$('#lbd').html('元');
			$('#ubd').html('元');
		}
		disableDater();
	}
}

function autoChanger(){
	if($('#periodSel').val()>0){
		$('#nav_down').show();
		$('.brr').show();
		
		$('#sorterSel').prop('disabled', false);
		$('#sorterPnl').show();
		
		$('#directorSel').prop('disabled', false);
		$('#directorPnl').show();
		
		$('#classorSel').prop('disabled', false);
		$('#classorPnl').show();
		
		$('#lboundSel').prop('disabled', false);
		$('#lboundPnl').show();
		
		$('#uboundSel').prop('disabled', false);
		$('#uboundPnl').show();
		
		$('#keywordSel').prop('disabled', false);
		$('#keywordPnl').show();
	}else{
		$('#nav_down').hide();
		$('.brr').hide();
		
		$('#sorterSel').prop('disabled', true);
		$('#sorterSel').val('InterestRate');
		$('#lbd').html('%');
		$('#ubd').html('%');
		$('#sorterPnl').hide();
		
		$('#directorSel').prop('disabled', true);
		$('#directorSel').val('minus');
		$('#directorPnl').hide();
		
		$('#classorSel').prop('disabled', true);
		$('#classorSel').val('none');
		$('#classorPnl').hide();
		
		$('#lboundSel').prop('disabled', true);
		$('#lboundPnl').hide();
		
		$('#uboundSel').prop('disabled', true);
		$('#uboundPnl').hide();
		
		$('#keywordSel').prop('disabled', true);
		$('#keywordPnl').hide();
		
		$('#lboundSel').val('');
		$('#uboundSel').val('');
		$('#keywordSel').val('');
		disableDater();
	}
}

var ifDater=false;

function enableDater(){
	if(!ifDater){
		enableDaterInner();
	}
}

function enableDaterInner(){
	$('#lboundSel').attr('placeholder','年/月/日');
	$('#uboundSel').attr('placeholder','年/月/日');
	$("#lboundSel").datepicker();
	$("#uboundSel").datepicker();
	ifDater=true;
}

function disableDater(){
	if(ifDater){
		disableDaterInner();
	}
}

function disableDaterInner(){
	$('#lboundSel').attr('placeholder','');
	$('#uboundSel').attr('placeholder','');
	$('#lboundSel').datepicker( "destroy" );
	$('#lboundSel').removeClass("hasDatepicker");
	$('#uboundSel').datepicker( "destroy" );
	$('#uboundSel').removeClass("hasDatepicker");
	ifDater=false;
}

